{
	"root": true,
	"extends": [
		"wikimedia/client",
		"wikimedia/jquery",
		"wikimedia/mediawiki"
	],
	"env": {
		"commonjs": true,
		"jest": true
	},
	"globals": {
		"Backbone": "readonly",
		"_": "readonly",
		"moment": "readonly"
	},
	"rules": {
		"compat/compat": "warn",
		"no-jquery/no-global-selector": "off"
	}
}
